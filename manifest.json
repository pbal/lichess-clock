{
    "manifest_version": 2,
    "name": "LiChess Animated Clock",
    "description": "This extension shows an animated clock while playing game on Lichess.org",
    "version": "1.1",
    "options_ui": {
        "page": "./options/options.html",
        "chrome_style": false

    },
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://lichess.org/*",
                "https://lichess.org/*"
            ],
            "css": [
                "./yellow.css"
            ]
        }
    ]
}